//convert number to String
function numberToString(num) {
  // Return a string of the number here!
return num.toString();
}

//Return Negative
function makeNegative(num) {
  // Code?
  if(num>0){
  return -num;
  }
  if(num==0)
  {
  return 0;
  }
  if(num<0)
  {
  return num;
  }

}

//Remove First and Last Character
